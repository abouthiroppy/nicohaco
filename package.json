{
  "name": "NicoHaco",
  "main": "dist/main.js",
  "author": "abouthiroppy <git@about-hiroppy.com http://about-hiroppy.com",
  "version": "1.0.0",
  "license": "MIT",
  "description": "NicoBoxのPC版です",
  "homepage": "",
  "engines": {
    "node": ">= 7.0.0"
  },
  "scripts": {
    "test": "jest --coverage",
    "flow": "flow",
    "lint": "npm run lint:js && npm run lint:css",
    "start": "webpack-dev-server",
    "build": "NODE_ENV=production npm run build:main && NODE_ENV=production npm run build:render",
    "build:main": "webpack --config ./webpack.main.config.js",
    "build:render": "webpack",
    "lint:js": "eslint .",
    "electron": "npm run build:main && electron .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "lint:css": "stylelint src/**/*.css",
    "postinstall": "npm run flow-typed",
    "flow-typed": "flow-typed install",
    "test:watch": "jest --watch --coverage",
    "package": "npm run build && npm run package:osx && npm run package:win",
    "package:osx": "build --mac --x64 && npm run package:sign",
    "package:sign": "sudo electron-osx-sign ./docs/mac/nicohaco.app --provisioning-profile=nicohaco.provisionprofile",
    "package:win": "build --win --x64"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.0.0",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.0.0",
    "conventional-changelog-cli": "^1.3.1",
    "css-loader": "^0.28.1",
    "dotenv": "^4.0.0",
    "electron": "^1.7.12",
    "electron-builder": "^19.52.1",
    "electron-devtools-installer": "^2.2.0",
    "enzyme": "^3.3.0",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.15.0",
    "eslint-config-sky": "^1.6.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "flow-bin": "^0.63.1",
    "flow-status-webpack-plugin": "^0.1.7",
    "flow-typed": "^2.1.2",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.0.6",
    "jsdom": "^11.5.1",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.3",
    "postcss-mixins": "^6.0.0",
    "postcss-reporter": "^5.0.0",
    "postcss-smart-import": "^0.7.6",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "next",
    "redux-devtools": "^3.4.0",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "stylelint-config-sky": "^1.0.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dashboard": "^1.0.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "@nicohaco/electron-window-manager": "^1.1.1",
    "classnames": "^2.2.5",
    "colorthief": "^2.0.2",
    "convert-keys": "^1.2.0",
    "dexie": "^2.0.1",
    "electron-google-analytics": "0.0.24",
    "nico-api": "^0.14.0",
    "nico-value": "^0.1.1",
    "node-vibrant": "^3.0.0",
    "photon": "https://github.com/connors/photon",
    "rc-slider": "^8.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-icons": "^2.2.7",
    "react-loading": "^1.0.3",
    "react-modal": "^3.1.10",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "4.2.2",
    "react-router-redux": "next",
    "react-select": "^1.2.1",
    "redux": "^3.6.0",
    "redux-saga": "^0.16.0"
  },
  "build": {
    "appId": "com.electron.nicohaco",
    "productName": "nicohaco",
    "files": [
      "dist",
      "index.html",
      "package.json",
      "build"
    ],
    "directories": {
      "output": "docs"
    },
    "mac": {
      "target": "dmg",
      "identity": null
    },
    "win": {
      "icon": "images/nicohaco.ico",
      "target": "nsis"
    }
  }
}
